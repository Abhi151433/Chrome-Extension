{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "My First Extension with a change",
    "version": "1.0",
    "content_scripts" : [
        {
            "js" : ["scripts/content1.js"],
            "matches" : [
                "https://www.linkedin.com/feed/*"
            ]
        }
    ],
    "action": {
        "default_popup": "hello.html"
      },
    "background": {
        "service_worker" : "background.js",
        "type" : "module"
      },
    "permissions": ["activeTab"]
}